Cell[BoxData[
 RowBox[{"Manipulate", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Block", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"above", "=", 
        RowBox[{"{", "}"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"below", "=", 
        RowBox[{"{", "}"}]}], ",", "\[IndentingNewLine]", "mPt", ",", 
       "\[IndentingNewLine]", "intEval"}], "\[IndentingNewLine]", "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "split", " ", "the", " ", "partial", " ", "data", " ", "list", " ", 
       "into", " ", "\"\<above\>\"", " ", "and", " ", "\"\<below\>\"", " ", 
       "point", " ", "lists"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"k", "=", "1"}], ",", 
        RowBox[{"k", "\[LessEqual]", "n"}], ",", 
        RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"mPt", "=", 
          RowBox[{"dataPts", "[", 
           RowBox[{"[", "k", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"f", "[", 
             RowBox[{"mPt", "[", 
              RowBox[{"[", "1", "]"}], "]"}], "]"}], "\[LessEqual]", 
            RowBox[{"mPt", "[", 
             RowBox[{"[", "2", "]"}], "]"}]}], ",", "\[IndentingNewLine]", 
           RowBox[{"below", "=", 
            RowBox[{"Append", "[", 
             RowBox[{"below", ",", "mPt"}], "]"}]}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"above", "=", 
            RowBox[{"Append", "[", 
             RowBox[{"above", ",", "mPt"}], "]"}]}]}], "\[IndentingNewLine]", 
          "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"intEval", "=", 
          RowBox[{"N", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Dimensions", "[", "below", "]"}], "[", 
