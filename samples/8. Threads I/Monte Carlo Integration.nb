Cell[CellGroupData[{

Cell["1.3.2 Now compute the integral", "Subsubsection",
 CellChangeTimes->{{3.814791893313785*^9, 3.814791908036165*^9}, {
  3.814791943920376*^9, 
  3.8147919632908*^9}},ExpressionUUID->"3f2ff210-7054-497d-87ac-b06c6d3df70c"],

Cell[BoxData[
 RowBox[{
  RowBox[{"MonteCarloIntegrate", "[", 
   RowBox[{
   "xMin_", ",", "xMax_", ",", "yMin_", ",", "yMax_", ",", "maxCount_", ",", 
    "printSteps_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Block", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "x", ",", "y", ",", "\[IndentingNewLine]", "k", ",", 
      "\[IndentingNewLine]", 
      RowBox[{"belowCount", "=", "0"}]}], "\[IndentingNewLine]", "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"k", "=", "1"}], ",", 
       RowBox[{"k", "\[LessEqual]", "maxCount"}], ",", 
       RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"x", ",", "y"}], "}"}], "=", 
         RowBox[{"ptInRange", "[", 
          RowBox[{"xMin", ",", "xMax", ",", "yMin", ",", "yMax"}], "]"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"If", " ", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"f", "[", "x", "]"}], " ", "\[GreaterEqual]", "y"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"belowCount", "++"}]}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", " ", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Mod", "[", 
            RowBox[{"k", ",", "printSteps"}], "]"}], "\[Equal]", "0"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"Print", "[", 
           RowBox[{
           "\"\<k=\>\"", ",", "k", ",", "\"\< \[RightArrow] \>\"", ",", 
            "belowCount", ",", "\"\</\>\"", ",", "k", ",", "\"\< = \>\"", ",", 
            RowBox[{"N", "[", 
             RowBox[{"belowCount", "/", "k"}], "]"}], ",", 
            "\"\<, \[Integral] = \>\"", ",", 
            RowBox[{"N", "[", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"xMax", "-", "xMin"}], ")"}], "*", 
              RowBox[{"(", 
               RowBox[{"yMax", "-", "yMin"}], ")"}], "*", 
              RowBox[{"belowCount", "/", "k"}]}], "]"}]}], "]"}]}], 
         "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], 
     ";", "\[IndentingNewLine]", " ", 
     RowBox[{"N", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"xMax", "-", "xMin"}], ")"}], "*", 
       RowBox[{"(", 
        RowBox[{"yMax", "-", "yMin"}], ")"}], "*", 
       RowBox[{"belowCount", "/", "maxCount"}]}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.494862372848488*^9, 3.494862730276347*^9}, {
   3.494862775045068*^9, 3.494862780239637*^9}, {3.4948628549669333`*^9, 
